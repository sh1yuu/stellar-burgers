var BurgerConstructorElement = (0,react.memo)(function (_a) {
    var ingredient = _a.ingredient, index = _a.index, totalItems = _a.totalItems;
    var dispatch = store_useDispatch();
    var handleMoveDown = function () {
        dispatch(reorderIngredientInBurgerConstructor({ from: index, to: index + 1 }));
    };
    var handleMoveUp = function () {
        dispatch(reorderIngredientInBurgerConstructor({ from: index, to: index - 1 }));
    };
    var handleClose = function () {
        dispatch(removeIngredientFromConstructor(index));
    };
    return ((0,jsx_runtime.jsx)(BurgerConstructorElementUI, { ingredient: ingredient, index: index, totalItems: totalItems, handleMoveUp: handleMoveUp, handleMoveDown: handleMoveDown, handleClose: handleClose }));
