var burger_constructor_BurgerConstructor = function () {
    /** TODO: взять переменные constructorItems, orderRequest и orderModalData из стора */
    var dispatch = store_useDispatch();
    var navigate = dist_useNavigate();
    var user = store_useSelector(selectorData);
    var constructorItems = store_useSelector(selectorBurgerIngredients);
    var orderRequest = store_useSelector(selectorBurgerLoading);
    var orderModalData = store_useSelector(selectorBurgerOrder);
    var onOrderClick = function () {
        if (!constructorItems.bun || orderRequest)
            return;
        if (!user) {
            navigate('/login');
        }
        else {
            var ingredientsId = burger_constructor_spreadArray(burger_constructor_spreadArray([], constructorItems.ingredients.map(function (i) { return i._id; }), true), [
                constructorItems.bun._id
            ], false);
            dispatch(orderBurger(ingredientsId));
        }
    };
    var closeOrderModal = function () {
        dispatch(resetConstructor());
    };
    var price = (0,react.useMemo)(function () {
        return (constructorItems.bun ? constructorItems.bun.price * 2 : 0) +
            constructorItems.ingredients.reduce(function (s, v) { return s + v.price; }, 0);
    }, [constructorItems]);
    return ((0,jsx_runtime.jsx)(BurgerConstructorUI, { price: price, orderRequest: orderRequest, constructorItems: constructorItems, orderModalData: orderModalData, onOrderClick: onOrderClick, closeOrderModal: closeOrderModal }));
};

